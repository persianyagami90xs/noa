STRUCT iovec

TYPE off_t
TYPE ssize_t

SYSCALL 1 read
RET ssize_t
ARG fildes : int
ARG iov : const struct iovec *
ARG iovcnt : int
ARG offset : off_t
ARG flags : int
END

SYSCALL 2 write
RET ssize_t
ARG fildes : int
ARG iov : const struct iovec *
ARG iovcnt : int
ARG offset : off_t
ARG flags : int
END

SYSCALL 3 openat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG oflag : int
ARG mode : mode_t
END

SYSCALL 4 close
PUBLIC 
RET int
ARG fildes : int
END

SYSCALL 5 mkdirat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG mode : mode_t
END

SYSCALL 6 fstatat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG buf : struct stat *
ARG flag : int
END

SYSCALL 7 mkfifoat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG mode : mode_t
END

SYSCALL 8 renameat
PUBLIC
RET int
ARG oldfd : int
ARG old : const char *
ARG newfd : int
ARG new : const char *
END

SYSCALL 9 fchmodat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG mode : mode_t
ARG flag : int
END

SYSCALL 10 pselect
PUBLIC
RET int
ARG nfds : int
ART readfds : fd_set *
ARG writefds : fd_set *
ARG errorfds : fd_set *
ARG timeout : const struct timespec *
ARG sigmask : const sigset_t *
END

SYSCALL 11 ioctl
RET int
ARG fildes : int
ARG request : unsigned long
ARG arg : void *
END

SYSCALL 12 posix_openpt
PUBLIC
RET int
ARG oflag : int
END

SYSCALL 13 unlinkat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG flag : int
END

SYSCALL 14 waitid
PUBLIC
RET int
ARG idtype : idtype_t
ARG id : id_t
ARG infop : siginfo_t *
ARG options : int
END

SYSCALL 15 linkat
PUBLIC
RET int
ARG fd1 : int
ARG path1 : const char *
ARG fd2 : int
ARG path2 : const char *
ARG flat : int
END
