STRUCT iovec

TYPE off_t
TYPE ssize_t

SYSCALL 1 read
RET ssize_t
ARG fildes : int
ARG iov : const struct iovec *
ARG iovcnt : int
ARG offset : off_t
ARG flags : int
END

SYSCALL 2 write
RET ssize_t
ARG fildes : int
ARG iov : const struct iovec *
ARG iovcnt : int
ARG offset : off_t
ARG flags : int
END

SYSCALL 3 openat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG oflag : int
ARG mode : mode_t
END

SYSCALL 4 close
RET int
ARG fildes : int
END

SYSCALL 5 mkdirat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG mode : mode_t
END

SYSCALL 6 fstatat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG buf : struct stat *
ARG flag : int
END

SYSCALL 7 mkfifoat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG mode : mode_t
END

SYSCALL 8 renameat
PUBLIC
RET int
ARG oldfd : int
ARG old : const char *
ARG newfd : int
ARG new : const char *
END

SYSCALL 9 fchmodat
PUBLIC
RET int
ARG fd : int
ARG path : const char *
ARG mode : mode_t
ARG flag : int
END
